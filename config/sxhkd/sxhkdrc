#SYSTEM CONTROL
super + d
	setxkbmap -layout dvorak

super + shift + d
	setxkbmap -layout dvorak -variant pl

super + shift + s
	power_menu


#APPS
super + c
	code

super + b
	brave

super + t
	st

super + e
	st -e ranger

super + r
  dmenu_run -sb '#228b22'

super + v
	clipmenu


